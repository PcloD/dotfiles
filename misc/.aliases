alias ll="ls -la"
alias ag="ag --path-to-ignore ~/.ignore"
alias sz="source ~/.zshrc"
alias ez="vim ~/.zshrc"
alias lp="fzf --preview='cat {}' --preview-window=right:70%:wrap +x"
alias sudovim="sudo -e"
alias weather="curl wttr.in/boston"
alias ypl="yarn --pure-lockfile"

# ignore stupid eog errors
alias eog="eog 2>/dev/null "

alias f="ranger"

alias gdt="git difftool"
alias gcm="git commit -m "
alias gpf="git push --force"
alias gdno="git diff --name-only "


